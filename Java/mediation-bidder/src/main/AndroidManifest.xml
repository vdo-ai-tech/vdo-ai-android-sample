        <activity
            android:name=".AdManager.BannerActivity"
            android:exported="false" />
        <activity
            android:name=".AdManager.InterstitialActivity"
            android:exported="false" />
        <activity
            android:name=".AdManager.RewardedVideoActivity"
            android:exported="false" />
        <activity
            android:name=".AdManager.RewardedInterstitialActivity"
            android:exported="false" />
        <activity
            android:name=".Admob.BannerActivity"
            android:exported="false" />
        <activity
            android:name=".Admob.InterstitialActivity"
            android:exported="false" />
        <activity
            android:name=".Admob.RewardedVideoActivity"
            android:exported="false" />
        <activity
            android:name=".Admob.RewardedInterstitialActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-3940256099942544~3347511713" />
    </application>

</manifest>